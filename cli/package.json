{
  "name": "@lagoon/lagoon-cli",
  "version": "0.1.0",
  "description": "Lagoon Commandline Tool",
  "main": "./dist/cli.js",
  "bin": {
    "io": "./dist/cli.js"
  },
  "files": ["dist"],
  "scripts": {
    "prepublish": "in-publish && yarn run dist || not-in-publish",
    "preversion": "yarn test",
    "version": "yarn run dist",
    "postversion": "git push && git push --tags",
    "update-schema": "node scripts/updateSchema.js",
    "clean": "rimraf dist",
    "dist": "yarn run clean && yarn run build",
    "lint": "eslint src",
    "format":
      "prettier --trailing-comma all --single-quote --write 'src/**/*.js' && yarn run lint -- --fix",
    "test": "yarn run lint && jest",
    "test:watch": "jest --watch",
    "build": "babel src -d dist",
    "watch": "babel --watch=src -d dist",
    "execute": "node -r babel-core/register src/cli.js",
    "lint-staged": "lint-staged",
    "flow": "flow check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amazeeio/lagoon.git"
  },
  "author": "Patrick Stapfer <patrick@amazee.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amazeeio/lagoon/issues"
  },
  "homepage": "https://github.com/amazeeio/lagoon#readme",
  "devDependencies": {
    "@lagoon/commons": "3.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^20.0.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "dotenv-extended": "^2.0.1",
    "eslint": "4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.61.0",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.3",
    "node-fetch": "^1.7.2",
    "prettier": "^1.8.2",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.1.0",
    "co": "^4.6.0",
    "graphql": "^0.11.7",
    "in-publish": "^2.0.0",
    "inquirer": "^3.2.2",
    "js-yaml": "^3.9.1",
    "prettier-eslint": "^8.2.5",
    "ramda": "^0.24.1",
    "ssh2": "^0.5.5",
    "ssh2-streams": "^0.1.19",
    "table": "^4.0.1",
    "untildify": "^3.0.2",
    "util.promisify": "^1.0.0",
    "yargs": "^10.0.3"
  },
  "jest": {
    "rootDir": "src"
  }
}
